{
  "name": "gatsby-starter-tailwind-seo-social",
  "version": "1.0.0",
  "description": "Gatsby starter project styled with Tailwind and configured for social sharing and SEO.",
  "author": "Kyle Galbraith",
  "dependencies": {
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-link": "next",
